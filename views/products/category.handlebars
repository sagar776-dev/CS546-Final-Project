<article class="product-list">
    <ul>
        {{#each manufactort_List}}
        <li>
            <form method="POST">
                <input type="submit" name="ManufacturerOfCategoryName" value="{{this}}" />
            </form>

        </li>
        {{/each}}
        <table>
            {{#each product_List}}
            <tr>
                <td>
                    <img src={{ pictures.[0] }} alt={{pictures.[0]}} width="100" height="100">
                </td>
                <td>
                    <a href="{{category}}/{{_id}}">{{name}}</a>
                </td>
            </tr>
            {{/each}}
        </table>
    </ul>
</article>